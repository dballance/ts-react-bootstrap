{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "interface-name": [true, "never-prefix"]
  },
  "linterOptions": {
    "exclude": ["./config/**", "./spec/**"]
  }
}
