{
  "name": "starter",
  "version": "0.0.0",
  "description": "Typescript React Starter",
  "main": "index.js",
  "author": "Drew Ballance <drew.ballance@hexagon.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "webpack --config ./config/webpack.common.ts",
    "build:dev": "webpack --config ./config/webpack.dev.ts",
    "build:prod": "webpack --config ./config/webpack.prod.ts",
    "lint": "tslint -p .",
    "prettier": "prettier --config ./prettier.json --write \"./**/*.ts\"",
    "start": "webpack-dev-server --config ./config/webpack.dev.ts --open",
    "test": "jest",
    "tdd": "jest --watch"
  },
  "dependencies": {
    "core-js": "^2.5.6",
    "normalize.css": "^8.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.1.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^4.1.7",
    "@types/webpack-dev-server": "^2.9.5",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-merge": "^4.1.3",
    "autoprefixer": "^9.0.1",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "postcss": "^7.0.1",
    "postcss-loader": "^2.1.5",
    "prettier": "1.13.7",
    "react-hot-loader": "^4.2.0",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.3.0",
    "ts-node": "^7.0.0",
    "tslib": "^1.9.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "~2.9.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(tsx?)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/app).*\\.(test|spec)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "./spec/test.ts",
    "setupFiles": [
      "./spec/polyfills/requestAnimationFrame.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
