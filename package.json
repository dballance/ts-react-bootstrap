{
  "name": "starter",
  "version": "0.0.0",
  "description": "Typescript React Starter",
  "main": "index.js",
  "author": "Drew Ballance <drew.ballance@hexagon.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "webpack --config ./config/webpack.common.ts",
    "build:dev": "webpack --config ./config/webpack.dev.ts",
    "build:prod": "webpack --config ./config/webpack.prod.ts",
    "lint": "tslint -p .",
    "prettier": "prettier --config ./prettier.json --write \"./**/*.ts\"",
    "start": "webpack-dev-server --config ./config/webpack.dev.ts --open",
    "test": "jest",
    "tdd": "jest --watch"
  },
  "dependencies": {
    "core-js": "^2.5.1",
    "normalize.css": "^8.0.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.6",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.4.6",
    "@types/ramda": "^0.25.13",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-hot-loader": "^3.0.4",
    "@types/webpack": "^3.0.10",
    "@types/webpack-env": "^1.13.1",
    "@types/webpack-merge": "^4.1.0",
    "autoprefixer": "^8.0.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.7",
    "prettier": "1.10.2",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.0.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslib": "^1.8.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "^3.5.3",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.3.3",
    "typescript": "~2.7.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(tsx?)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/app).*\\.(test|spec)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "./spec/test.ts",
    "setupFiles": [
      "./spec/polyfills/requestAnimationFrame.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
