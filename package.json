{
  "name": "starter",
  "version": "0.0.0",
  "description": "Typescript React Starter",
  "main": "index.js",
  "author": "Drew Ballance <drew.ballance@hexagon.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "webpack --config ./config/webpack.common.ts",
    "build:dev": "webpack --config ./config/webpack.dev.ts",
    "build:prod": "webpack --config ./config/webpack.prod.ts",
    "lint": "tslint -p .",
    "prettier": "prettier --config ./prettier.json --write \"./**/*.{ts,tsx}\"",
    "start": "webpack-dev-server --config ./config/webpack.dev.ts --open",
    "test": "jest",
    "tdd": "jest --watch"
  },
  "dependencies": {
    "core-js": "^2.6.1",
    "normalize.css": "^8.0.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@types/enzyme": "^3.1.10",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^4.4.21",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-merge": "^4.1.3",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.7",
    "postcss-loader": "^3.0.0",
    "prettier": "1.15.3",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "~3.2.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(tsx?)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/app).*\\.(test|spec)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "./spec/test.ts",
    "setupFiles": [
      "./spec/polyfills/requestAnimationFrame.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
